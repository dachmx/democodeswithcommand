<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.txazo.domain.User" table="user" catalog="forssh">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="username" length="20"/>
		<property name="password" length="20"/>
		<property name="realname" length="50"/>
		<property name="age"/>
		<property name="sex" length="2"/>
		<property name="address" length="50"/>
	</class>
</hibernate-mapping>